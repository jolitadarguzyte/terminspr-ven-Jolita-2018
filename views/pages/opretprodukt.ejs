                <h1>Opret Produkt</h1>
                <form id="form">
                    <div class="form-group">
                        <label class="col-form-label" for="formGroupExampleInput">Type</label>
                        <input type="text" class="form-control" id="type" placeholder="Products type ">
                    </div>
                    <div class="form-group">
                        <label class="col-form-label" for="formGroupExampleInput">Mærker</label>
                        <input type="text" class="form-control" id="mærker" placeholder="Products mærker">
                    </div>
                    <div class="form-group">
                      <label class="col-form-label" for="formGroupExampleInput">Url</label>
                      <input type="file" class="form-control" id="url" placeholder="Billede" value="billede.jpg">
                  </div>
                    <div class="form-group">
                        <label class="col-form-label" for="formGroupExampleInput">Pris</label>
                        <input type="number" class="form-control" id="pris" placeholder="Pris">
                    </div>
                    <div class="form-group">
                        <label class="col-form-label" for="formGroupExampleInput">Farve</label>
                        <input type="text" class="form-control" id="farve" placeholder="Products farve">
                    </div>
                    <div class="form-group">
                        <label class="col-form-label" for="formGroupExampleInput">Beskrivelse</label>
                        <input type="text" class="form-control"  id="beskrivelse" placeholder="Products beskrivelse">
                    </div>
                    <div class="form-group">
                        <label class="col-form-label" for="formGroupExampleInput">Model</label>
                        <input type="text" class="form-control" id="model" placeholder="Products model">
                    </div>
                    <button type="submit" class="btn btn-primary" id="opretProdukt">Bekræft</button>
                </form>
